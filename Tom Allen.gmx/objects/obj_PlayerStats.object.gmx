var tempHP = global.hp;
for (var i=0; i &lt; ceil(global.maxhp/4); i++) {
    if (tempHP &gt; 4) {
        draw_sprite_ext(spr_heartcontainer, 0, 100+i*36, 60, 1, 1, 0, c_white, 1);
        tempHP -= 4;   
    }
    else if (tempHP &gt; 0) {
        draw_sprite_ext(spr_heartcontainer, (4-tempHP), 100+i*36, 60, 1, 1, 0, c_white, 1); 
        tempHP = 0;   
    }
    else if (tempHP == 0) {
        draw_sprite_ext(spr_heartcontainer, 4, 100+i*36, 60, 1, 1, 0, c_white, 1);  
    }
    
}
    
draw_text(40, 80, floor(global.bufftime/60)); 
