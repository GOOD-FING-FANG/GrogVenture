            <kind>1</kind>
            <string>///Step through the dialogue tree

if(count == 0)
{
    if(keyboard_check_pressed(vk_space))
    {       
        scr_text(dialogue[count,0],spd,dialogue[count,1]);
        count++;
    }
}

else if(instance_exists(obj_text))
{
    if(obj_text.done)
    {
        if(keyboard_check_pressed(vk_space))
        {
            with (obj_text) instance_destroy();
                
            if(array_height_2d(dialogue) &gt; count)
            {
                scr_text(dialogue[count,0],spd,dialogue[count,1]);
                count++;
            }
            else if(array_height_2d(dialogue) == count)
            {
                //global.dialogueDone = true;
                //obj_player.state = states.active;
                    
                dialogue_complete = true;                    
            }
        }
    }
}

else if(cutscene_complete)
{
    obj_player.stage = 0;
    obj_woodrow.stage = 0;
    
    obj_player.state = states.active;
}

else if(dialogue_complete)
{
    if(obj_player.stage == 0)
    {
        if(obj_player.x &lt; 1024)
        {
            if(obj_player.sprite_index != spr_tom_walk)
            {
                obj_player.image_index = 0;
                obj_player.sprite_index = spr_tom_walk;
                obj_player.image_speed = 0.6;
            }
            obj_player.x += 5;
        }
        else
        {
            obj_player.sprite_index = spr_tom_idle;
            obj_player.stage = 1;
        }
    }
    if(obj_woodrow.stage == 0)
    {
        if(obj_woodrow.x &lt; 1024)
        {
            obj_woodrow.x += 4;
        }
        else
        {
            obj_woodrow.stage = 1;
        }
    }
    if(obj_woodrow.stage == 1 &amp;&amp; obj_player.stage == 1)
    {
        cutscene_complete = true;
        room_goto_next();
    }
}
</string>
